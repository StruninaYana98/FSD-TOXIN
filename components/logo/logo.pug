mixin logo
  if colorlogo
    div.logo.logo_color
      img.logo__left-wing.logo__left-wing_color(src="../components/logo/color-left-wing.svg")
      img.logo__right-wing.logo__right-wing_color(src="../components/logo/color-right-wing.svg")
  else
    div.logo
      img.logo__left-wing(src="../components/logo/left-wing.svg")
     
      img.logo__right-wing(src="../components/logo/right-wing.svg")
